<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<GridPane prefWidth="-1.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/2.2" fx:controller="scalafxml.demo.unitconverter.UnitConverterPresenter">
    <children>
        <Label text="Conversion type:" GridPane.columnIndex="0" GridPane.rowIndex="0" />
        <Label text="From:" GridPane.columnIndex="0" GridPane.rowIndex="1" />
        <Label text="To:" GridPane.columnIndex="0" GridPane.rowIndex="2" />
        <Button mnemonicParsing="false" onAction="#onClose" text="Close" GridPane.columnIndex="1" GridPane.halignment="RIGHT" GridPane.rowIndex="3" />
        <ComboBox fx:id="types" maxWidth="1.7976931348623157E308" GridPane.columnIndex="1" GridPane.halignment="LEFT" GridPane.rowIndex="0">
            <GridPane.margin>
                <Insets bottom="3.0" left="3.0" right="3.0" top="3.0" fx:id="x1" />
            </GridPane.margin>
        </ComboBox>
        <TextField fx:id="from" prefWidth="200.0" GridPane.columnIndex="1" GridPane.margin="$x1" GridPane.rowIndex="1" />
        <TextField fx:id="to" editable="false" prefWidth="200.0" GridPane.columnIndex="1" GridPane.margin="$x1" GridPane.rowIndex="2" />
    </children>
    <columnConstraints>
        <ColumnConstraints halignment="LEFT" hgrow="SOMETIMES" maxWidth="129.0" minWidth="10.0" prefWidth="111.0" />
        <ColumnConstraints halignment="RIGHT" hgrow="ALWAYS" maxWidth="149.0" minWidth="10.0" prefWidth="149.0" />
    </columnConstraints>
    <padding>
        <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
    </padding>
    <rowConstraints>
        <RowConstraints maxHeight="30.0" minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        <RowConstraints maxHeight="31.0" minHeight="10.0" prefHeight="31.0" vgrow="SOMETIMES" />
        <RowConstraints maxHeight="30.0" minHeight="10.0" prefHeight="29.0" vgrow="SOMETIMES" />
        <RowConstraints maxHeight="30.0" minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
    </rowConstraints>
</GridPane>